{
  "name": "Metrics",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "clean": "rm -rf node_modules ; npm install ; (cd ios ; rm -rf Pods ; rm Podfile.lock ; pod install --repo-update)",
    "deploy": "react-native run-ios --configuration Release --device \"Cellular Device\"",
    "dev": "npm run start --silent",
